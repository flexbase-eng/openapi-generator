{
  "include": ["../platform/openapi/Identity.yaml"],
  "sharedTemplates": ["./templates/client/typescript/**/*.hbs"],
  "generate": {
    "routes": {
      "target": "./output/{api}/routes/{name}.ts",
      "template": "./templates/client/typescript/routes.hbs"
    },
    "models": {
      "target": "./output/{api}/models/{name}.ts",
      "template": "./templates/client/typescript/models.hbs"
    }
  },
  "prettier": true,
  "tags": true,
  "flatten": true,
  "references": true,
  "debugPath": "./output/{api}/ast/"
}
