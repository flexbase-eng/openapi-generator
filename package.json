{
  "name": "@flexbase/openapi-generator",
  "version": "3.0.11",
  "description": "Open API code generator",
  "author": {
    "name": "Flexbase Technologies",
    "email": "hello@flex.one",
    "url": "https://flex.one"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flexbase-eng/openapi-generator/issues"
  },
  "homepage": "https://github.com/flexbase-eng/openapi-generator#readme",
  "keywords": [
    "openapi",
    "swagger",
    "generator"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flexbase-eng/openapi-generator.git"
  },
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "engines": {
    "node": ">=20.8.1"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "openapi-generator": "dist/index.js"
  },
  "type": "module",
  "packageManager": "pnpm@9.5.0",
  "scripts": {
    "build": "turbo run build",
    "build:ci": "turbo run build:ci",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:ci": "turbo run test:ci",
    "format": "turbo run format",
    "changeset": "changeset && changeset version",
    "clean": "rimraf ./**/dist --glob",
    "clean:bin": "turbo run clean:bin",
    "clean:all": "rimraf ./**/node_modules ./**/.turbo ./**/dist --glob",
    "start": "node --inspect --enable-source-maps ./apps/cli/dist/index.js",
    "packr": "turbo run packr"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@flexbase/packr": "^1.2.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "turbo": "^2.3.3",
    "@vitest/coverage-v8": "^2.1.8",
    "typescript": "^5.7.3",
    "vite": "6.0.7",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^2.1.8"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "arrowParens": "avoid",
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 150,
    "overrides": [
      {
        "files": [
          "*.hbs"
        ],
        "options": {
          "singleQuote": false,
          "parser": "glimmer"
        }
      }
    ]
  }
}
