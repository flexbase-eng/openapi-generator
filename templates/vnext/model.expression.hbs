{{~#compare type "===" "object"}}
    {
    {{#each properties}} {{>model.expression .}}{{/each}}
    }
{{/compare~}}

{{~#compare type "===" "property"}}
    {{name}}{{^required}}{{^defaultValue}}?{{/defaultValue}}{{/required}}: {{>model.expression definition}};
{{/compare~}}

{{~#compare type "===" "array"}}
    {{>model.expression definition}}[]
{{/compare~}}

{{~#compare type "===" "string"}}string{{/compare~}}
{{~#compare type "===" "boolean"}}boolean{{/compare~}}
{{~#compare type "===" "number"}}number{{/compare~}}
{{~#compare type "===" "integer"}}number{{/compare~}}

{{~#compare type "===" "reference"}}{{& resolveReference2 .}}{{/compare~}}

{{~#compare type "===" "composite"}}
    {{#if definitions}}{{#forEach definitions}}{{>model.expression .}}{{#unless isLast}}&{{/unless}}{{/forEach}}{{/if}}
{{/compare~}}

{{~#compare type "===" "union"}}
    {{#if definitions}}{{#forEach definitions}}{{>model.expression .}}{{#unless isLast}}|{{/unless}}{{/forEach}}{{/if}}
{{/compare~}}

{{~#compare type "===" "parameter"}}
    {{>model.expression definition}}
{{/compare~}}